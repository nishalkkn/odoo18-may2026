<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--    Contact Us page template-->
    <template id="mars_contact" inherit_id="website.contactus"
              name="Contactus">
        <xpath expr="//section[hasclass('s_title')]"
               position="replace">
        </xpath>
        <xpath expr="//section[hasclass('s_text_block')]"
               position="replace">
            <section class="contact_classic s_text_block contact-custom">
                <section class="contact_content_ct">
                    <div class="wrapper">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-5 col-md-6 col-12">
                                    <div class="address_wrapper_ct">
                                        <div class="address_ct">
                                            <h4>3646 Byers Lane</h4>
                                            <p>Mogi das Cruzes</p>
                                            <p>SÃ£o Paulo 08735-030</p>
                                            <p>Cell:
                                                <a href="tel:+4733378901">
                                                    +47 333 78 901
                                                </a>
                                            </p>
                                        </div>
                                        <div class="address_ct">
                                            <h4>422 Duncan Avenue</h4>
                                            <p>New Yorks</p>
                                            <p>NY New York 10016</p>
                                            <p>Cell:
                                                <a href="tel:+1 917-413-0508">
                                                    +1 917-413-0508
                                                </a>
                                            </p>
                                        </div>
                                    </div>
                                    <div class="social_ct">
                                        <h4>SOCIAL</h4>
                                        <div class="social_icon_ct">
                                            <a href="#">
                                                <span class="fa fa-dribbble"/>
                                            </a>
                                            <a href="#">
                                                <span class="fa fa-google-plus-g"/>
                                            </a>
                                            <a href="#">
                                                <span class="fa fa-linkedin-in"/>
                                            </a>
                                            <a href="#">
                                                <span class="fa fa-twitter"/>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-7 col-md-6 col-12">
                                    <section
                                            class="s_website_form pt16 pb16"
                                            data-vcss="001"
                                            data-snippet="s_website_form">
                                        <div class="post_replay_ct">
                                            <form class="contact-form_ct o_mark_required"
                                                  id="contactus_form"
                                                  action="/website/form/"
                                                  method="post"
                                                  enctype="multipart/form-data"
                                                  data-model_name="mail.mail"
                                                  data-success-mode="redirect"
                                                  data-success-page="/contactus-thank-you"
                                                  data-pre-fill="true">
                                                <div class="row">
                                                    <div class="col-12 s_website_form_field">
                                                        <div class="form-group">
                                                            <input type="text"
                                                                   class="form-control s_website_form_input s_website_form_mark"
                                                                   id="contact1"
                                                                   name="name"
                                                                   placeholder="NAME *"
                                                                   required=""
                                                                   data-fill-with="name"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 s_website_form_field">
                                                        <div class="form-group">
                                                            <input type="email"
                                                                   class="form-control s_website_form_input s_website_form_mark"
                                                                   id="contact3"
                                                                   name="email_from"
                                                                   required=""
                                                                   placeholder=" EMAIL *"
                                                                   data-fill-with="email"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 s_website_form_field">
                                                        <div class="form-group">
                                                            <input type="text"
                                                                   class="form-control s_website_form_input s_website_form_mark"
                                                                   id="contact5"
                                                                   name="subject *"
                                                                   required=""
                                                                   placeholder="SUBJECT"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 s_website_form_field">
                                                        <div class="form-group pt-4">
                                                            <textarea name="description"
                                                                      class="txt form-control s_website_form_input s_website_form_mark"
                                                                      cols=""
                                                                      rows="10"
                                                                      style="width: 100%;"
                                                                      id="contact6"
                                                                      placeholder="DESCRIPTION"/>
                                                        </div>
                                                    </div>
                                                    <div class="mb-0 py-2 col-12 s_website_form_field s_website_form_dnone">
                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact7">
                                                            <span class="s_website_form_label_content">Email To</span>
                                                        </label>
                                                        <div class="col-sm">
                                                            <input id="contact7" type="hidden" class="form-control s_website_form_input" name="email_to"/>
                                                        </div>
                                                    </div>
                                                </div>
                                                    <div class="mb-0 py-2 col-12 s_website_form_submit"
                                                         data-name="Submit Button">
                                                        <div class="s_website_form_label"/>
                                                        <a href="#"
                                                           role="button"
                                                           class="btn btn-black ml-3 btn-lg s_website_form_send">
                                                            SEND NOW
                                                        </a>
                                                        <span id="s_website_form_result"/>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </section>
                                </div>
                            </div>
                            <div class="map">
                                <div class="map-responsive">
                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d15658.881983874671!2d75.87971722644363!3d11.134181973268767!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3ba651d1fb20b40b%3A0xd15c44b13dfe431c!2sThenhipalam%2C%20Kerala!5e0!3m2!1sen!2sin!4v1598930664895!5m2!1sen!2sin"
                                            width="100%" height="450"
                                            frameborder="0" style="border:0;"
                                            allowfullscreen=""
                                            aria-hidden="false"
                                            tabindex="0"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </section>
        </xpath>
    </template>
</odoo>
